<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <div class="container-fluid">
    <%= link_to "#", class: "navbar-brand" do %>
      <%= image_tag "data:image/jpeg;base64," %>
    <% end %>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav me-auto">
          <li class="nav-item active">
            <%= link_to "Lists", "#", class: "nav-link" %>
          </li>
          <li class="nav-item">
            <%= link_to "Bookmars", "#", class: "nav-link" %>
          </li>
      </ul>
    </div>
  </div>
</div>
<div>
<h1>Lists🧐</h1>
<h2><%= @list.name %></h2>

<div>
  <%=link_to "Back to lists🔙", lists_path %>

  <%=button_to "Destroy this list🗑️", @list, method: :destroy %>
</div>
<br>
<% @list.bookmarks.each do |bookmark| %>
  <%= bookmark.comment %>
  <%= image_tag bookmark.movie.poster_url, style: 'width: 100px;' %>
<% end %>

<%= button_to "Add a new bookmark", new_list_bookmark_path(@list), method: :get %>
</div>

